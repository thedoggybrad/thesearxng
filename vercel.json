{
  "functions": {
    "api/php/index.php": {
      "runtime": "vercel-php@0.7.3",
      "maxDuration": 60
    },
    "api/php/search.php": {
      "runtime": "vercel-php@0.7.3",
      "maxDuration": 60
    },
    "api/php/autocompleter.php": {
      "runtime": "vercel-php@0.7.3",
      "maxDuration": 60
    }
  },
  "routes": [
    {
      "src": "/favicon/(.*)",
      "dest": "/favicon/$1"
    },
    {
      "src": "/search",
      "dest": "api/php/search.php"
    },
    {
      "src": "/autocompleter",
      "dest": "api/php/autocompleter.php"
    },
    {
      "src": "/(.*)",
      "dest": "api/php/index.php"
    }
  ]
}
